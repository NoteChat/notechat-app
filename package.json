{
  "name": "NoteChat",
  "version": "0.9.0",
  "description": "A efficient Text tool base on ChatGPT-4, Cluade",
  "main": "./out/main/index.js",
  "author": "161.ai",
  "homepage": "https://www.161.ai",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:web": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "build:api": "curl http://localhost:3000/api-json -o ./src/renderer/src/api/swagger.json && npx swagger-typescript-api --extract-response-body -p ./src/renderer/src/api/swagger.json -o ./src/renderer/src/api -n api.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@radix-ui/colors": "^0.1.9",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tryfabric/martian": "^1.2.4",
    "@vscode/codicons": "^0.0.33",
    "any-text": "^1.2.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "electron-updater": "^5.3.0",
    "highlight.js": "^11.8.0",
    "i18next": "^23.2.11",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "quill": "^1.3.7",
    "quill-markdown-shortcuts": "^0.0.10",
    "quill-markdown-toolbar": "^0.1.3",
    "quill-paste-smart": "^1.4.12",
    "rc-select": "^14.5.2",
    "rc-table": "^7.32.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.0.2",
    "react-loader-spinner": "^5.3.4",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.1",
    "react-split-pane": "^0.1.92",
    "rehype-highlight": "^6.0.0",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "sass": "^1.63.6",
    "socket.io-client": "^4.7.1",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.4",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.2",
    "electron": "^24.6.2",
    "electron-builder": "^24.4.0",
    "electron-vite": "^1.0.24",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "less": "^4.1.3",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swagger-typescript-api": "^12.0.4",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
    "vite": "^4.4.0"
  },
  "build": {
    "productName": "NoteChat"
  }
}
